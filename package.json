  "author": "Jimmy Laurent",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/JimmyLaurent/torrent-search-api.git"
  },
  "dependencies": {
    "cloudscraper": "^4.6.0",
    "lodash": "^4.17.21",
    "request": "^2.88.2",
    "string-format": "^2.0.0",
    "x-ray-scraper": "^3.0.6"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "install": "^0.13.0",
    "jest": "^29.3.1",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.0.3",
    "nock": "^13.2.9",
    "prettier": "^2.7.1"
  },
  "jest": {
